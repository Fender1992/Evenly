# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Plaid
PLAID_CLIENT_ID=your-plaid-client-id
PLAID_SECRET=your-plaid-secret
PLAID_ENV=sandbox
PLAID_ENCRYPTION_SECRET=your-32-byte-secret

# Stripe
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Sentry
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
EXPO_PUBLIC_SENTRY_DSN=https://your-expo-sentry-dsn@sentry.io/project-id

# PostHog (optional)
POSTHOG_API_KEY=phc_your-posthog-api-key
EXPO_PUBLIC_POSTHOG_API_KEY=phc_your-posthog-api-key
EXPO_PUBLIC_POSTHOG_ENABLED=false

# Mobile API Base (optional override)
EXPO_PUBLIC_API_URL=http://localhost:3001

# Server
PORT=3001
NODE_ENV=development
